	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#A7E1C3",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/position/position",
			"iconPath": "static/position.png",
			"selectedIconPath": "static/positions.png",
			"text": "职业"
		}, {
			"pagePath": "pages/personnel/personnel",
			"iconPath": "static/personnel.png",
			"selectedIconPath": "static/personnels.png",
			"text": "人才"
		}, {
			"pagePath": "pages/my/my",
			"iconPath": "static/mine.png",
